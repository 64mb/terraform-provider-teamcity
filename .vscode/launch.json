  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch test function",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceRoot}/teamcity",
      "args": [
        "-test.v"
      ],
      "env": {
        "TF_ACC":1,
        "TEAMCITY_URL": "localhost:8111",
        "TEAMCITY_USER": "admin",
        "TEAMCITY_PASSWORD": "admin",
        "DEBUG": 1,
        "TF_LOG": "Debug"
      }
    },
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${fileDirname}",
      "env": {
        "TF_ACC":1
      },
      "args": [
        "-test.v"
      ],
      "showLog": true,
      "trace": "verbose"
    }
  ]
}